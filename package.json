{
  "name": "template-fullstack",
  "version": "3.0.0",
  "description": "project template for the React-Express-MySQL cursus in Wild Code School",
  "homepage": "https://github.com/wildcodeschool/js-template-fullstack#readme",
  "bugs": {
    "url": "https://github.com/wildcodeschool/js-template-fullstack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wildcodeschool/js-template-fullstack.git"
  },
  "config": {
    "cli": "npm"
  },
  "scripts": {
    "postinstall": "cross-env-shell \"husky install && cd ./frontend && $npm_package_config_cli install \"",
    "dev": "cross-env-shell \"cd ./frontend && $npm_package_config_cli run dev\"",
    "lint": "cross-env-shell \"cd ./frontend && $npm_package_config_cli run lint \"",
    "fix": "cross-env-shell \"cd ./frontend && $npm_package_config_cli run fix \""
  },
  "keywords": [],
  "author": "Wild Code School",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.13.0",
    "react-table": "^7.8.0",
    "sass": "^1.63.4",
    "style-components": "^0.1.0",
    "use-sound": "^4.0.1"
  }
}
